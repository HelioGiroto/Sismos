   grep 'Alaska' sismos5+ | grep -E '\|7\|' > Alaska.txt
   grep 'Alaska' sismos5+ | grep -E '\|7\.' >> Alaska.txt
   grep 'Alaska' sismos5+ | grep -E '\|8\|' >> Alaska.txt
   grep 'Alaska' sismos5+ | grep -E '\|8\.' >> Alaska.txt
   
